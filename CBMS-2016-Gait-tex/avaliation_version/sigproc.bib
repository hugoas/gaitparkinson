@Article{6678519,
  Title                    = {Near-Realistic Mobile Exergames With Wireless Wearable Sensors},
  Author                   = {Mortazavi, B. and Nyamathi, S. and Lee, S.I. and Wilkerson, T. and Ghasemzadeh, H. and Sarrafzadeh, M.},

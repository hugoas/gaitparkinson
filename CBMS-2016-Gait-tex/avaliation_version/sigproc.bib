@Article{Friedman2014,
  Title                    = {The Manumeter: A Wearable Device for Monitoring Daily Use of the Wrist and Fingers},
  Author                   = {Friedman, N. and Rowe, J.B. and Reinkensmeyer, D.J. and Bachman, M.},
  Journal                  = {Biomedical and Health Informatics, IEEE Journal of},
  Year                     = {2014},

  Month                    = {Nov},

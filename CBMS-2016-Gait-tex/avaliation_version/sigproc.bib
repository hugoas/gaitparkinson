@InProceedings{Marcelino:2008:ECA:1473252.1474515,
  Title                    = {Elder Care Architecture},
  Author                   = {Marcelino, Isabel and Barroso, Jo\~{a}o and Cruz, Jos\'{e} Bulas and Pereira, Ant\'{o}nio},
  Booktitle                = {Proceedings of the 2008 Third International Conference on Systems and Networks Communications},
  Year                     = {2008},

  Address                  = {Washington, DC, USA},
  Pages                    = {349--354},
  Publisher                = {IEEE Computer Society},

  Abstract                 = {As we observe society in our days, we can see that people live longer; this means that we have an older population. As we know, elder are more likely to have health issues. The special needs presented by elder are gaining a major concern from all of us. Another tendency of our society is the lack of time that people demonstrate and, as a consequence, the lack of time reflects when families can't take care of their own elders. Many solutions are being presented in order to solve this problem. Some of them are taking advantage of the new technology developments in body sensor network area. In this paper we proposed an architecture of a system called Elder Care. The Elder Care solution has two primary goals: monitoring vital signs, sending alerts to family and specialized help and provide a social network in order to wrap all elder to avoid social isolation.},
  Acmid                    = {1474515},
  Doi                      = {10.1109/ICSNC.2008.66},
  ISBN                     = {978-0-7695-3371-1},
  Numpages                 = {6},
  Owner                    = {leonardo},
  Review                   = {The Elder Care solution has two primary goals: - monitoring vital signs, sending alerts to family and specialized help - provide a social network in order to wrap all elder to avoid social isolation. ------------------------------------------------------------------------------------------------------------------------------- As an emerging problem, providing a better life for elder people and be able to reduce health costs, many solutions The major importance of solutions to deal with elder people is so extreme that there are innumerous projects in development referred in [16] and [17]: OLDES (Older People's e-services at home), CAALYX (Complete Ambient Assisting Living EXperiment) , K4CARE (Knowledge-based HomeCare eServices for an ageing Europe), ALADIN (Ambient Lighting Assistance for an Ageing Population), ENABLE (A wearable system supporting services to enable elderly people to live well, independently and at ease), SHARE-IT (Supported human autonomy for recovery and enhancement of cognitive and motor abilities using information technologies), EASY LINE The solutions mentioned above are focused on the physical well being of the patient, monitoring vital signs. Many elder live alone in their homes, isolated, lonely, with no activities what so ever. Arquitetura Proposta --------------------------------- The proposed architecture is divided in 3 primary modules: local monitoring, control center and virtual leisure room (visible in Figure 1). The local monitoring will be installed at patient‚Äôs home, getting vital signs and sending them to the control center that will be responsible for setting in motion the proper alerts and specialized help as a response to abnormal situations. 3.1. Local Monitoring -------------------------------- Local monitoring, represented in Figure 3, will be installed in elder people‚Äôs home. It will acquire vital signs from body wireless sensor, in a non invasive method, and send the abnormal information and samples of normal information to the control center through a communication network. The vital signs to monitor are: blood pressure, ocular pressure, ECG, fall detection and body fluids. This choice was made after some interviews to doctors and nurses staff that indicated these parameters as the more important to survey in elder people. They emphases fall detection as one of the most important parameters, indicating that a fall can mean that the elder had a breakdown and needs immediate assistance. 3.2 Control Center ---------------------------- The control center, represented in Figure 4, will receive the information from all local monitoring. Additionally to the fact of being an information repository, the control center manages all the alerts; meaning that, it is possible to compare current data with the patient record‚Äôs and check what parameters are abnormal and inform the family as well as activate proper emergency responses. This allows having a personalized response for each case because, besides the incoming alert, the control center has all the medical records and previous alerts of each patient. Comunication --------------------- The communications with National Health System is going to be developed using a standard protocol like HL7, DICOM, HIPAA, CEN/TC 251 (WGIII) - Security, Safety and Quality, ISO TC 215 (WG 4) - Security, IEEE P1157 Medical Data Interchange (MEDIX), IEEE P1073 Medical Information Bus (MIB) [18] [19], according to the country where the solution will be implemented. 3.3. Virtual Leisure Room --------------------------------------- As people are getting older, they have a propensity to isolate themselves at home mostly by physical movement difficulties. As a consequence, they start to feel sad and to be a burden to society by not being able to contribute somehow. The virtual leisure room promotes a social relationship between all elder from Elder Care solution in order to prevent social seclusion (Figure 5). The virtual leisure room is going to be conceived to cover most of elder, including people illiterate and with some motor disabilities but cannot be applied to elder that have degenerative mental illnesses, such as Alzheimer. The virtual leisure room will also offer the opportunity to the users to expose their doubts to doctors, nurses and medical staff. This is a benefit because elder people feel, many times, embarrassment to ask personally some questions to their own doctors. This feature will allow that users pose some questions without having to meet the doctor in person. With Elder Care solution we will be able to maintain elders in their homes, surveying their health and make them useful by interaction in the virtual leisure room. Conclus√£o ---------------- O trabalho foi realizado uma pesquisa qualitativa para a proposta de uma arquitetura: We followed a qualitative research with semi structured interviews in order to obtain the requirements necessary to the proposed architecture.},
  Timestamp                = {2011.07.31}
}

@InProceedings{mare:healthsec11,
  Title                    = {Adaptive security and privacy for {mHealth} sensing},
  Author                   = {Shrirang Mare and Jacob Sorber and Minho Shin and Cory Cornelius and David Kotz},
  Booktitle                = {USENIX Workshop on Health Security (HealthSec)},
  Year                     = {2011},
  Month                    = {August},
  Note                     = {Short paper.},

  Abstract                 = {As healthcare in many countries faces an aging population and rising costs, mobile Health (mHealth) sensing technologies promise a new opportunity. However, the privacy concerns associated with mHealth sensing are a limiting factor for their widespread adoption. The use of wireless body area networks pose a particular challenge. Although there exist protocols that provide a secure and private communication channel between two devices, the large transmission overhead associated with these protocols limit their application to low-power mHealth sensing devices. We propose an adaptive security model that enables use of privacy-preserving protocols in low-power mHealth sensing by reducing the network overhead in the transmissions, while maintaining the security and privacy properties provided by the protocols.},
  Copyright                = {the authors},
  Owner                    = {leonardo},
  Review                   = {Recent improvements in mobile computing and developments in miniature medical sensors have enabled mobile Health (mHealth) sensing.},
  Timestamp                = {2011.07.16}
}

